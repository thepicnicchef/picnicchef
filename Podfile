target 'picnicchef' do
  # Comment the next line if you don't want to use dynamic frameworks
  use_frameworks!

  # Pods for picnicchef
  pod 'Firebase/Analytics'
  pod 'Firebase/Auth'
  # add other pods as needed

  target 'picnicchefTests' do
    inherit! :search_paths
    # Pods for testing
    pod 'Firebase/Analytics' # if needed for tests
    pod 'Firebase/Auth'      # if needed for tests
  end

  target 'picnicchefUITests' do
    inherit! :search_paths
    # Pods for testing
    pod 'Firebase/Analytics' # if needed for UI tests
    pod 'Firebase/Auth'      # if needed for UI tests
  end

end

